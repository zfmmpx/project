public class HelloWorld2{
    public static void main(String[] args) {
    	boolean breakouet = false;
    	for (int i = 0; i < 10; i++) {
    		
    		for (int j = 0; j < 10; j++) {
    			System.out.println(i + ":" + j);
    			if(0 == j % 2) {
    				breakouet = true;
    				
    				break;
    			}
    		}
    		if (breakouet) //判断是否终止外部循环
    			break;
    	}
    }
}