import java.util.Scanner;
public class HelloWorld2 {
	public static void main(String[] arg) {
		System.out.println("请输入月份");
		Scanner s = new Scanner(System.in);
		int month = s.nextInt();
		String season;
		switch(month) {
		case 3:
		case 2:
		case 1:
			season = "srping";
		case 4:
		case 5:
		case 6:
			season = "summer";
		case 7:
		case 8:
		case 9:
			season = "fall";
		case 10:
		case 11:
		case 12:
			season = "winter";
		default:
			System.out.println("什么鬼");
		}
	}
}