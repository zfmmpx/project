import java.util.Scanner;
public class HelloWorld2 {
	public static void main(String[] arg) {
		Scanner s = new Scanner(System.in);
		System.out.println("请输入一个年份");
		int year = s.nextInt();
		String status = "";
		if (year % 4 == 0 & year % 100 != 0 | year % 400 == 0)
			status = "是闰年";
		else 
			status = "不是闰年";
		System.out.println(status);
	}	
}