public class HelloWorld{
	public static void main(String[] args) {
		float F;
		final float rate = 0.2f;
		int n = 100000;
		float p = 12000;
		float compoundInterestRate = 1;
		for(int i = 1; i < n; i++) {
			compoundInterestRate = compoundInterestRate + rate;
			p = p * compoundInterestRate;
			System.out.println("第" + i + "年总收入为:" + p);
			if (p > 1000*1000)
				break;
		}
	}
}